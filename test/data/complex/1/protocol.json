{
  "services": {
    "test1": {
      "version": 11,
      "transports": [],
      "actions": {
        "some1": "domain.some1",
        "some2": {
          "input": "domain.some2Input",
          "output": "domain.some2Output",
          "execute": "domain.some2Execute"
        },
        "some3": "domain.some3"
      }
    },
    "test2": {
      "version": 1,
      "transports": ["amqp"],
      "actions": {}
    },
    "test3": {
      "version": 123123123,
      "transports": ["amqp"],
      "actions": {
        "foo": "domain.foo"
      }
    }
  },

  "gateway": {
    "middlewares": ["mw.jwt", "mw.prepare"],
    "routes": [
      {
        "middlewares": ["mw.headers"],
        "path": "/",
        "aliases": {
          "GET /test": [],
          "POST /test": [],
          "PUT /test/:id": [],
          "PATCH /test/:id": [],
          "DELETE /test/:id": []
        }
      },
      {
        "path": "/some",
        "aliases": {
          "GET /1": ["test1.v11.some1"],
          "POST /2": ["test1.v11.some2"],
          "PATCH /3": ["test1.v11.some3"]
        }
      },
      {
        "path": "/products",
        "aliases": {}
      },
      {
        "middlewares": ["mw.apple", "mw.juice"],
        "path": "/some/apple/juice",
        "aliases": {
          "GET /": ["domain.apple.juice"],
          "POST /:id": ["mw.prepare", "domain.apple.juiceCreate"],
          "PATCH /some/:id": ["mw.prepare", "domain.apple.juiceUpdate"],
          "DELETE /some/123/zxc": ["test3.v123123123.foo"]
        }
      }
    ]
  },

  "cron": {
    "timezone": "Europe/Berlin",
    "disabled": false,
    "jobs": [
      {
        "name": "some1",
        "pattern": "*/15 * * * * *",
        "execute": "domain.cron.some1"
      },
      {
        "name": "some2",
        "pattern": "*/30 * * * *",
        "execute": "domain.cron.some2",
        "disabled": true
      }
    ]
  }
}
